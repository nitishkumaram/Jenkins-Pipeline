node{
    stage('Build'){
        cleanWs()
        git url: 'https://github.com/pipelineascodecourse/simple_java_project_with_sonarqube.git'

            withMaven(maven: 'Maven'){
                sh "mvn clean install"

                withSonarQubeEnv('MySonarQubeServer'){
                    sh "mvn sonar:sonar"
                }
            }
    }
}
